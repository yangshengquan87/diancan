.container {
  width: 100%;
  background: #f5f5f9;
}
.add{
  width: 100%;
  height: 200rpx;
}
.add image{
  display: block;
  width: 100%;
  height: 100%;
}
.getPrice {
  width: 100%;
  height: 280rpx;
  background: #fff;
  border-top: 1rpx solid #ddd;
}
.getPrice .hasPrice {
  width: 100%;
  height: 130rpx;
  padding: 50rpx 0;
  color: #333;
}
.getPrice .hasPrice text {
  display: block;
  width: 100%;
  font-size: 26rpx ;
  text-align: center;
  font-weight: bold;
}
.getPrice .hasPrice .price {
  width: 100%;
  font-weight: bold;
  font-size: 80rpx ;
  text-align: center;
}
.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 999;
  background-color: #fff;
}
.getPrice .need_input_price {
  width: 100%;
  height:260rpx;
  padding: 30rpx 0;
  color: #333;
}
.getPrice .need_input_price view {
  width: 210rpx;
  margin: 0 auto;
  font-size: 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.getPrice .need_input_price .shop{
  display:block;
  width:100%;
font-size:22rpx;
color:#999;
text-align:center;
margin:0 auto;
}
.getPrice .need_input_price view image {
  display: block;
  width: 60rpx;
  height: 60rpx;
}
.getPrice .need_input_price view text {
  display: block;
}
.getPrice .need_input_price input {
  width: 421rpx;
  height: 84rpx;
  margin: 20rpx auto;
  border: 1rpx solid #999;
  font-size: 26rpx;
  padding-left: 30rpx;
  border-radius: 5rpx;
  position:relative;
  z-index:100;
}
.discount_coupon {
  width: 100%;
  margin-top: 20rpx;
  background: #fff;
}
.discount_coupon .disct_info {
  width: 96%;
  padding: 0 2%;
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.discount_coupon .disct_info view {
  font-size: 28rpx ;
  color: #888;
}
.discount_coupon .disct_info view text {
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  border-radius: 30rpx;
  background: #ff6b12;
  color: #fff;
  margin-right: 20rpx ;
  font-size: 22rpx;
  text-align: center;
}
.discount_coupon .disct_info view:last-of-type {
  color: #ff6b12;
}
.discount_coupon .disct_info view:last-of-type image {
  width: 13rpx;
  height: 24rpx;
}
.tips .tip_title {
  width: 96%;
  padding: 0 2%;
  color: #333;
  font-size: 30rpx;
  margin: 25rpx 0;
}
.tips textarea {
  width: 96%;
  padding: 0 2%;
  height: 40rpx;
  margin-bottom: 20rpx;
  font-size: 22rpx;
  color: #333;
}
.pay_methods {
  width: 96%;
  margin-top: 20rpx;
  background: #fff;
  padding: 30rpx 2%;
}
.pay_methods .pay_title {
  color: #333;
  font-size: 30rpx;
}
.pay_methods .pay_check {
  width: 80%;
  margin: 0 auto;
  margin-top: 30rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.pay_methods .pay_check .pay_check_part {
  width: 254rpx;
  height: 88rpx;
  border: 1px solid #ddd;
  padding: 0rpx 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  border-radius: 10rpx;
}
.pay_methods .pay_check .pay_check_part image {
  width: 70rpx;
  height: 45rpx;
  display: block;
  margin-right: 20rpx;
}
.pay_methods .pay_check .pay_check_part text {
  font-size: 20rpx;
  color: #888;
}
.pay_methods .pay_check .pay_check_part:last-of-type image {
  width: 57rpx;
  height: 57rpx;
}
.pay_methods .pay_check .pay_active {
  border-color: #ff6b12;
}
.advertimets {
  width: 100%;
  background: #fff;
  margin-top: 20rpx;
}
.advertimets image {
  display: block;
  width: 100%;
  height: 200rpx;
}
.pay_bottom {
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 98rpx;
  line-height: 98rpx;
  display: flex;
  align-items: center;
  color: #fff;
}
.pay_bottom .pay_price {
  width: 76.8%;
  height: 100%;
  background: #393939;
  font-size: 38rpx;
  padding-left: 4%;
  box-sizing: border-box;
}
.pay_bottom .pay_btn {
  width: 23.2%;
  height: 100%;
  background: #ff6b12;
  font-size: 30rpx;
  text-align: center;
}
.mark {
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 999;
  background: rgba(0, 0, 0, 0.5);
  display: flex;
  justify-content: center;
  align-items: center;
}
.mark-body {
  background-color: #fff;
  width: 90%;
  height: 80%;
}
.mark-body .title {
  width: 100%;
  height: 120rpx;
  text-align: center;
  display: inline-block;
  font-size: 30rpx;
  color: #333;
  line-height: 120rpx;
  position: relative;
  border-bottom: 1rpx solid #333;
}
.mark-body .title .close {
  position: absolute;
  font-size: 40rpx;
  color: #333;
  right: 30rpx;
  vertical-align: middle;
}
.mark-body .cou {
  width: 100%;
  padding: 0rpx 3% 0rpx 3%;
  box-sizing: border-box;
}
.mark-body .cou .cou-container {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 200rpx;
  background: url("http://wangcong.fun/images/youhui_03.png") no-repeat;
  background-size: 100% 100%;
  margin: 20rpx 0;
}
.mark-body .cou .cou-container .left {
  flex: 1;
}
.mark-body .cou .cou-container .left .icon {
  color: #fffefe;
  font-size: 46rpx;
  letter-spacing: -40rpx;
}
.mark-body .cou .cou-container .left .mon {
  color: #fffefe;
  font-size: 166rpx;
  font-weight: 600;
  letter-spacing: -25rpx;
}
.mark-body .cou .cou-container .right-container {
  flex: 2.4;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.mark-body .cou .cou-container .right-container .center {
  flex: 1.3;
}
.mark-body .cou .cou-container .right-container .center .c-top {
  color: #fffefe;
  font-size: 34rpx;
}
.mark-body .cou .cou-container .right-container .center .c-bottom {
  color: #fffefe;
  font-size: 22rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.mark-body .cou .cou-container .right-container .right {
  flex: 1;
}
.mark-body .cou .cou-container .right-container .right .r-con {
  text-align: center;
}
.mark-body .cou .cou-container .right-container .right .r-con .text {
  color: #fffefe;
  font-size: 32rpx;
}
.mark-body .cou .cou-container .right-container .right .r-con .use-btn {
  width: 156rpx;
  height: 48rpx;
  color: #ff6b12;
  line-height: 48rpx;
  text-align: center;
  font-size: 24rpx;
  border-radius: 10rpx;
  margin: 0 auto;
  background: linear-gradient(to bottom, #feefe1, #fbd8c0, #f8c4a3);
}
.mark-body .cou .cou-container .right-container .right .r-con1 image {
  width: 160rpx;
  height: 160rpx;
}
.mark-body .cou .cou-container .right-container .right .r-con2 image {
  width: 160rpx;
  height: 160rpx;
}
.mark-body .cou .cou-container0 {
  background: url("http://wangcong.fun/images/youhui_03.png");
}
.mark-body .cou .cou-container1 {
  background: url("http://wangcong.fun/images/ticket_07.png");
}
.mark-body .cou .cou-container2 {
  background: url("http://wangcong.fun/images/ticket_07.png");
}
.charge_container{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index:10000;
}
.charge_content{
  width: 92%;
  height:590rpx;
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
   margin: auto;
  background: #fff;
  border-radius: 5rpx;
}
.tel_title {
  width: 97%;
  margin: 0 auto;
  height: 120rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  color: #333;
  font-weight: bold;
  position: relative;
  border-bottom: 1rpx solid #ddd;
  margin-bottom: 40rpx;
}
.tel_title text {
  position: absolute;
  top: 15%;
  right: 10rpx;
  color: #888;
  font-size: 40rpx;
}
.select_num{
  width: 100%;
  padding: 0 4%;
  box-sizing: border-box;
  display: flex;
  justify-content: space-around;
  align-items: center;

}
.select_num .num_part{
  padding: 27rpx 13rpx;
  color: #666;
  font-size: 26rpx;
  display: block;
  border: 1rpx solid #666;
  border-radius: 5rpx;
}
.select_num .num_part.active{
  background: #42adae;
  border: none;
  color: #fff;
}
.ipt_num{
   width: 100%;
  padding: 0 4%;
  box-sizing: border-box;
  display: flex;
    justify-content: space-around;
  align-items: center;
  margin-top:40rpx 
}
.ipt_num text{
  color: #333;
  font-size: 28rpx;
}
.ipt_num input{
  width: 490rpx;
  height: 80rpx;
  border: 1rpx solid #666;
  border-radius: 5rpx;
  font-size: 24rpx;
}
.tips{
   width: 100%;
  padding: 0 5%;
  box-sizing: border-box;
  text-align: left;
  color: #888;
  font-size: 20rpx;
  margin-top: 10rpx;
  
}
.btn_container {
  width: 92%;
  margin: 40rpx auto;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.btn_container view {
  width: 280rpx;
  height: 80rpx;
  color: #333;
  font-size: 30rpx;
  border-radius: 5rpx;
  line-height: 80rpx;
  text-align: center;
  box-sizing: border-box;
}
.btn_container view.cancel_btn {
  border: 1rpx solid #ddd;
}
.btn_container view.ensure {
  background: #ff6b12;
  color: #fff;
}
.empty_container {
  width: 200rpx;
  height: 190rpx;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}
.empty_container image {
  display: block;
  width: 175rpx;
  height: 100rpx;
  margin: 30rpx auto;
}
.empty_container text {
  display: block;
  width: 100%;
  text-align: center;
  font-size: 26rpx;
  color: #666;
}